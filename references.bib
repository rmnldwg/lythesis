	file = {Ludwig et al. - 2021 - A hidden Markov model for lymphatic tumor progress.pdf:C\:\\Users\\roman\\Zotero\\storage\\299NM4NN\\Ludwig et al. - 2021 - A hidden Markov model for lymphatic tumor progress.pdf:application/pdf},
