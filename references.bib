	note = {arXiv: 1109.4524},
	keywords = {Physics - Medical Physics},
	pages = {1203--1216},
	annote = {Comment: 17 pages, 4 figures, 1 table},
	file = {arXiv Fulltext PDF:C\:\\Users\\roman\\Zotero\\storage\\PQSQ6HXD\\Ramakrishnan et al. - 2012 - A Dynamic Programming Approach to Adaptive Fractio.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\roman\\Zotero\\storage\\2TPVRZ9V\\1109.html:text/html},
