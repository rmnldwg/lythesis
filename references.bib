	keywords = {Radiotherapy, Lymph node metastases, Lymphatic Metastasis, Head and Neck Neoplasms, Human Papillomavirus (HPV), Humans, Lymph Nodes, Neck dissection, Oropharyngeal Neoplasms, Oropharynx, p16, Papillomavirus Infections, Prevalence, Retrospective Studies, Squamous Cell Carcinoma of Head and Neck, head \& neck},
	pages = {122--129},
